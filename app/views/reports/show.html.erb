<div class="main-contents">  
  <div class="dailyReport">
    <p><%= @report.created_at.strftime("%Y-%m-%d %H:%M") %></p>
    <h1>報告者</h1>
      <p><%= @report.user.name %></p>
    <h1>行先</h1>
      <p><%= @report.client %></p>
    <h1>面会者</h1>
      <p><%= @report.client_person %></p>
    <h1>用件</h1>
      <p><%= @report.business %></p>
    <h1>結果</h1>
      <p><%= @report.result %></p>
    <h1>備考</h1>
      <p><%= @report.note %></p>
    <div align="right">
      <%= link_to "編集", edit_report_path(@report.id), method: :get %>
      <%= link_to "削除", report_path(@report.id), method: :delete %>
    </div>
  </div>
</div>